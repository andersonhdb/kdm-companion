{
  "name": "kdm-gear-companion",
  "version": "3.1.0",
  "private": true,
  "devDependencies": {
    "expo-cli": "^2.4.1",
    "husky": "^1.2.0",
    "jest-date-mock": "^1.0.6",
    "jest-expo": "^29.0.0",
    "lint-staged": "^8.1.0",
    "node-fetch": "^2.3.0",
    "prettier": "^1.15.2",
    "react-dom": "^16.5.1",
    "react-native-scripts": "1.14.0",
    "react-test-renderer": "16.6.3"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "precommit": "lint-staged"
  },
  "jest": {
    "preset": "jest-expo",
    "cacheDirectory": ".jest",
    "setupFiles": [
      "jest-date-mock"
    ],
    "errorOnDeprecated": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|expo-.*|react-navigation.*|sentry-expo|@shoutem|react-native.*)/)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/coverage/",
      "__snapshots__/",
      "/src/data/"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@shoutem/theme": "^0.11.0",
    "@shoutem/ui": "ingoclaro/ui#set-statusbar-color-optional",
    "expo": "^29.0.0",
    "mobx": "4.x.x",
    "mobx-react": "5.x.x",
    "mobx-state-tree": "^3.0.2",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.0",
    "react": "16.3.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-29.0.0.tar.gz",
    "react-native-fit-image": "^1.5.4",
    "react-native-iap": "^2.3.19",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-modal": "^7.0.0",
    "react-native-simple-stepper": "^2.0.0",
    "react-navigation": "2.12.x",
    "sentry-expo": "^1.10.0"
  }
}
