{
  "name": "kdm-gear-companion",
  "version": "4.7.0",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "babel-preset-expo": "^5.1.1",
    "expo-cli": "^2.18.5",
    "husky": "^2.3.0",
    "jest-date-mock": "^1.0.7",
    "jest-expo": "^32.0.1",
    "lint-staged": "^8.1.7",
    "node-fetch": "^2.6.0",
    "prettier": "^1.17.1",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6"
  },
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node node_modules/jest/bin/jest.js --watch",
    "precommit": "lint-staged"
  },
  "jest": {
    "preset": "jest-expo",
    "cacheDirectory": ".jest",
    "setupFiles": [
      "jest-date-mock"
    ],
    "errorOnDeprecated": true,
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|expo-.*|@react-navigation.*|react-navigation-.*|sentry-expo|@shoutem|react-native.*)/)"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/android/",
      "/coverage/",
      "__snapshots__/",
      "/src/data/"
    ]
  },
  "lint-staged": {
    "*.{js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@ingoclaro/react-native-fit-image": "^1.5.4",
    "@shoutem/theme": "^0.11.3",
    "@shoutem/ui": "ingoclaro/ui#set-statusbar-color-optional",
    "expo": "^32.0.3",
    "mobx": "5.9.4",
    "mobx-react": "5.4.4",
    "mobx-state-tree": "^3.14.0",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.0",
    "react": "16.5.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-32.0.0.tar.gz",
    "react-native-draggable-flatlist": "^1.1.5",
    "react-native-iap": "^2.5.0",
    "react-native-markdown-renderer": "^3.2.8",
    "react-native-modal": "^11.0.1",
    "react-native-screens": "^1.0.0-alpha.22",
    "react-native-simple-stepper": "^2.1.3",
    "react-native-swipe-list-view": "^1.5.5",
    "react-navigation": "^3.11.0",
    "sentry-expo": "~1.12.0"
  }
}
